{
  "name": "p2panda-js",
  "version": "0.4.0",
  "description": "All the things a panda needs",
  "main": "lib/index.js",
  "browser": "lib/index.min.js",
  "types": "lib/types/index.d.ts",
  "scripts": {
    "build": "cross-env NODE_ENV=production run-s clear wasm wasm:types webpack",
    "clear": "rimraf ./lib ./wasm",
    "docs": "typedoc src/index.ts",
    "lint": "eslint --ext .ts .",
    "typecheck": "tsc --noEmit",
    "test": "cross-env NODE_ENV=development run-s clear wasm test:jest",
    "test:jest": "jest --coverage=$npm_config_coverage",
    "test:watch": "nodemon --watch '../p2panda-rs/src/*' --watch './src/*' --exec 'npm test' --ext js,ts,json",
    "wasm": "./scripts/compile-wasm.sh",
    "wasm:types": "cp ./wasm/node/index.d.ts ./src/wasm",
    "webpack": "run-p webpack:web webpack:node --aggregate-output",
    "webpack:node": "webpack --config-name node --color",
    "webpack:web": "webpack --config-name web --color"
  },
  "engines": {
    "node": ">= v12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/p2panda/p2panda.git"
  },
  "contributors": [
    "adz <x1d@adz.garden>",
    "cafca <cafca@001.land>",
    "sandreae <contact@samandreae.com>",
    "sophiiistika <sophiiistika@mailbox.org>"
  ],
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/p2panda/p2panda/issues"
  },
  "homepage": "https://github.com/p2panda/p2panda#readme",
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.9",
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/register": "^7.18.6",
    "@tsconfig/node12": "^1.0.11",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/debug": "^4.1.7",
    "@types/fetch-headers": "^2.0.0",
    "@types/jest": "^28.1.3",
    "@types/node": "^18.0.0",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-webpack-plugin": "^3.2.0",
    "jest": "^28.1.2",
    "mock-apollo-client": "^1.2.0",
    "nodemon": "^2.0.18",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.1",
    "ts-transform-paths": "^2.0.3",
    "tsconfig-paths": "^4.0.0",
    "typedoc": "^0.23.2",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "debug": "^4.3.4",
    "fetch-headers": "^2.0.0",
    "graphql": "^16.5.0",
    "node-fetch": "^2.6.7"
  }
}
