                "schema": {
                    "type": "object",
                    "properties": {
                        "entryHashSkiplink": {
                            "oneOf": [
                                {
                                    "type": "null"
                                },
                                {
                                    "type": "string",
                                    "minLength": 132,
                                    "maxLength": 132,
                                    "pattern": "[0-9a-f]{132}"
                                }
                            ]
                        },
                        "entryHashBacklink": {
                            "oneOf": [
